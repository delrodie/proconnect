{% extends 'base.html.twig' %}

{% block title %}Demandeur{% endblock %}

{% block body %}
    <h1>Demandeur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ demandeur.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ demandeur.code }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ demandeur.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ demandeur.prenom }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ demandeur.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ demandeur.telephone }}</td>
            </tr>
            <tr>
                <th>Profession</th>
                <td>{{ demandeur.profession }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ demandeur.adresse }}</td>
            </tr>
            <tr>
                <th>Media</th>
                <td>{{ demandeur.media }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ demandeur.slug }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ demandeur.createdAt ? demandeur.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_backend_demandeur_index') }}">back to list</a>

    <a href="{{ path('app_backend_demandeur_edit', {'id': demandeur.id}) }}">edit</a>

    {{ include('backend_demandeur/_delete_form.html.twig') }}
{% endblock %}
